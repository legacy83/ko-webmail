{
  "name": "trsenna/requirejs-minimal",
  "description": "RequireJS Minimal Project Template",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/trsenna/requirejs-minimal/issues",
    "source": "https://github.com/trsenna/requirejs-minimal"
  },
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/trsenna/gozma14"
    }
  ],
  "require": {
    "npm-asset/bootstrap": "3.3.*",
    "npm-asset/jquery": "3.1.*",
    "npm-asset/knockout": "3.4.*",
    "npm-asset/requirejs": "2.3.*",
    "npm-asset/requirejs-text": "2.0.*",
    "npm-asset/sammy": "0.7.*"
  },
  "require-dev": {
    "trsenna/gozma14": "0.6.0"
  },
  "config": {
    "process-timeout": 1800
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "web/vendor/npm",
      "bower-asset-library": "web/vendor/bower"
    }
  }
}